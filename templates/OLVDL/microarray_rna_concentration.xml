<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE OLVDL PUBLIC "-//OLVDL 0.4//EN" "http://doctype.open-lims.org/olvdl.dtd" >
<OLVDL>
	<HEAD>
		<ID>10001</ID>
		<TITLE>RNA concentration</TITLE>	
	</HEAD>
	<BODY>
		<FORMAT>
			<LINE>
				<PRINT value="RNA-Purification:"></PRINT>
			<FIELD name="method" default="RNeasy Kit"></FIELD>				
			</LINE>
			<LINE>               
				<PRINT value="&nbsp;"></PRINT>
			</LINE> 
			<LINE>
				<PRINT value="RNA-Concentration:" format="bold"></PRINT>
			</LINE>				
			<LINE>               
				<PRINT value="&nbsp;"></PRINT>
			</LINE> 
			<EACH var="this.item.sample.list.typeof.c1">
				<LINE>
					<PRINT var="item.each.getName" format="italic"></PRINT>                           
				</LINE>
				<LINE>
					<PRINT value="&#181;g/ml RNA:"></PRINT>
					<FIELD name="rna_con" length="8" vartype="float"></FIELD>
				</LINE>
				<LINE>
					<PRINT value="OD [high]260[/high]/[low]280[/low]:"></PRINT>
					<FIELD name="260to280" length="8" vartype="float"></FIELD>
				</LINE>
				<LINE>               
					<PRINT value="&nbsp;"></PRINT>
				</LINE> 
			</EACH>                               
		</FORMAT> 
	</BODY>             
</OLVDL>
